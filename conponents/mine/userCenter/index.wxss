/* conponents/mine/userCenter/index.wxss */
.main{
  padding:78rpx 74rpx;
}
.title{
  height: 100rpx;
  line-height: 74rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.bottom .information{
  background-image: url('data:image/png;base64,');
  background-size: 100% 110%;
  width: 462rpx;
  height: 112rpx;
  margin: 0 27rpx;
  border-top-right-radius: 25rpx;
  border-top-left-radius: 25rpx;
  color: #fff;
  font-size: 24rpx;
  padding: 47rpx 0 0 87rpx; 
  overflow: hidden;
}
.bottom .information .headPortrait{
  width: 91rpx;
  height: 91rpx;
  border-radius: 15rpx;
  background: white;
  float: left;
  margin: -20rpx 0 0 -50rpx;
}
.bottom .information .nickName{
  color: rgb(255, 255, 255);
  font-size: 26rpx;
  font-family: PingFang-SC-Blod;
  display: block;
  margin: -20rpx 0 0 50rpx;
}
.bottom .information .phoneNumber{
  color: rgb(255, 255, 255);
  font-size: 26rpx;
  font-family: PingFang-SC-Blod;
  display: block;
  margin: 20rpx 0 0 50rpx;
}
.bottom> image{
  margin: -30rpx 0 0 -22rpx;
  width: 650rpx;
  height: 480rpx;
}
.bottom .menu{
  border-radius: 25rpx; 
  width: 460rpx;
  height: 465rpx;
  padding: 0rpx 86rpx;
  position: absolute;
  top: 230rpx;
  left: 60rpx;
}
.bottom .menu .menu-item{
  display: flex;
  font-size: 26rpx;
  height: 106rpx;
  line-height: 106rpx;
  border-bottom: 1px solid #c3c3c3;
}
.bottom .menu .menu-item:last-of-type{
  border: none;
}
.bottom .menu .menu-item text{
  flex: 1;
}
.bottom .menu .menu-item image{
  width: 40rpx;
  height: 40rpx;
  margin: 35rpx 6rpx 0 0 ;
}
navigator{
  width: 100%;
}
.bottom .menu .menu-item .right{
  width: 14rpx;
  height: 25rpx;
  margin: 40rpx 0 0 0 ;
  float: right;
}
.main>image{
  width: 232rpx;
  height: 56rpx;
  position: absolute;
  bottom: 70rpx;
  right: 256rpx;
}