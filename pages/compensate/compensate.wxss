.carTest{
  position:relative;
  font-weight: bold;
  margin: 33rpx 0 0 99rpx;
  top: 33rpx;
}
.wrap{
  position: relative;
  width: 100%;
  /* margin: 0 0 0 5%; */
  top: 50rpx;
}
 
.checkbox-con{
  margin-top: 40rpx;
  text-align: center
}
.checkbox{
  border: 1rpx solid #CECECE;
  /* border-radius: 5rpx; */
  display: inline-block;
  margin: 0 10rpx 20rpx 0;
  position: relative;
  width: 120rpx;
  height: 120rpx;
  background: #c7c7c7;
  border-radius: 50%;
  line-height: 120rpx;
  margin-right: 40rpx;
  font-size: 16px;
  color: #595959;
}
.checked{
  color: white;
  background: #37aa4a;
  border: 1rpx solid #37aa4a;
}
.checkbox checkbox{
  display: none
}
.checked-img{
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 0;
  right: 0
}
.test{
  display: block;
  width: 135rpx;
  position: relative;
  /* margin:80rpx 0 0 0; */
  font-weight: bold;
  color: #333;
  font-size: 14px !important;
}
.other{
  position: relative;
  width: 80%;
  margin: 3% 10%;
  height: 110rpx;
  /* top: 40rpx; */
  border: 1rpx solid #c7c7c7;
  background: #e0e0e0;
  color: #333;
  display: none;
}
.camera{
  position: relative;
  width: 80rpx;
  height: 80rpx;
  display: none;
  margin: 50rpx 100rpx;
  top: 30rpx;
}
.chose{
  display: block;
}
.submit{
  position: fixed;
  width: 80%;
  margin: 0 0 0 10%;
  background: #37aa4a;
  color: white;
  border-radius: 46rpx;
  bottom: 100rpx;
}