.mapCoverButton{
  width: 526rpx;
  height: 88rpx;
  background-color: #37aa4a;
  border-radius: 44rpx;
  display: flex;
  align-items: center;
  color: #fff;
  font-size: 26rpx;
  position: absolute;
  left: 50%;
